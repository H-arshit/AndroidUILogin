<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.harshit.apiuse.Result">



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/image_user_profile"
        android:layout_centerInParent="true"
        android:text="Click" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/upload_image"
        android:text="Upload"

        android:layout_centerHorizontal="false"
        android:layout_alignBottom="@+id/image_user_profile" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/image_user_profile_view"/>



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/festival_list"
        android:text="festval"
        android:layout_below="@+id/image_user_profile"
        android:layout_toLeftOf="@+id/image_user_profile"
        android:layout_toStartOf="@+id/image_user_profile"
        android:layout_marginTop="27dp" />

</RelativeLayout>
